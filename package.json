{
  "name": "ts-express",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "lint": "eslint --fix ./",
    "prebuild": "npm run clean && tslint -c tslint.json -p tsconfig.json --fix",
    "build:parcel": "parcel build src/public/js/main.ts -d dist/public/js",
    "build": "tsc && npm run copy-assets && npm run build:parcel",
    "dev:build": "npm run build && node dist/index.js",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:build",
    "initdb": "ts-node tools/initdb",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "test": "jest --coverage"
  },
  "author": {
    "name": "David Neal",
    "email": "david@reverentgeek.com",
    "url": "https://reverentgeek.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^23.3.5",
    "@types/materialize-css": "^1.0.5",
    "@types/node": "^10.12.0",
    "@types/pg": "^7.4.11",
    "@types/shelljs": "^0.8.0",
    "@types/vue": "^2.0.0",
    "eslint": "^5.7.0",
    "eslint-config-leankit": "^4.5.0",
    "eslint-plugin-typescript": "^0.12.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "parcel-bundler": "^1.10.3",
    "rimraf": "^2.6.2",
    "shelljs": "^0.8.2",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.2.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3",
    "typescript-eslint-parser": "^20.0.0"
  },
  "dependencies": {
    "@okta/oidc-middleware": "^1.0.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "fs-extra": "^7.0.0",
    "materialize-css": "^1.0.0",
    "pg-monitor": "^1.0.0",
    "pg-promise": "^8.5.0",
    "vue": "^2.5.17"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  }
}
