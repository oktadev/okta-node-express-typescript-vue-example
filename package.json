{
  "name": "guitar-inventory",
  "version": "1.0.0",
  "description": "Sample Node.js application built with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "parcel": "parcel build src/public/js/main.ts -d dist/public/js",
    "build": "npm-run-all clean lint tsc copy-assets parcel",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "node .",
    "initdb": "ts-node tools/initdb",
    "test": "jest --coverage"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "author": {
    "name": "David Neal",
    "email": "david@reverentgeek.com",
    "url": "https://reverentgeek.com"
  },
  "keywords": [],
  "license": "Apache-2.0",
  "dependencies": {
    "@okta/oidc-middleware": "^5.0.1",
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.7",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "materialize-css": "^1.0.0",
    "pg": "^8.7.1",
    "pg-promise": "^10.11.1",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^23.3.10",
    "@types/materialize-css": "^1.0.11",
    "@types/node": "^16.11.12",
    "@types/pg": "^8.6.1",
    "@types/shelljs": "^0.8.9",
    "@types/vue": "^2.0.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.4.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "ts-jest": "^27.1.0",
    "ts-node": "^7.0.1",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2"
  }
}
