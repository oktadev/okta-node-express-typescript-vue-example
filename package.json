{
  "name": "guitar-inventory",
  "version": "1.0.0",
  "description": "Sample Node.js application built with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "parcel": "parcel build src/public/js/main.ts -d dist/public/js",
    "build": "npm-run-all clean lint tsc copy-assets parcel",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "node .",
    "initdb": "ts-node tools/initdb",
    "test": "jest --coverage"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "author": {
    "name": "David Neal",
    "email": "david@reverentgeek.com",
    "url": "https://reverentgeek.com"
  },
  "keywords": [],
  "license": "Apache-2.0",
  "dependencies": {
    "@okta/oidc-middleware": "^1.0.2",
    "axios": "^0.21.1",
    "dotenv": "^6.1.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "materialize-css": "^1.0.0",
    "pg": "^7.7.1",
    "pg-promise": "^8.5.2",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^23.3.10",
    "@types/materialize-css": "^1.0.5",
    "@types/node": "^10.12.10",
    "@types/pg": "^7.4.11",
    "@types/shelljs": "^0.8.0",
    "@types/vue": "^2.0.0",
    "fs-extra": "^7.0.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.10.3",
    "rimraf": "^2.6.2",
    "shelljs": "^0.8.3",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  }
}
